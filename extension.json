{
	"name": "WeirdGloopMessages",
	"version": "1.0.1",
	"author": [
		"[https://twitter.com/jaydenkieran Jayden Bailey]",
		"TehKittyCat",
		"..."
	],
	"url": "https://weirdgloop.org",
	"descriptionmsg": "weirdgloopmessages-desc",
	"license-name": "GPL-2.0+",
	"type": "other",
	"MessagesDirs": {
		"WeirdGloopMessages": [
			"i18n/weirdgloop"
		],
		"WeirdGloopOverrides": [
			"i18n/weirdgloopoverrides"
		],
		"WeirdGloopTemporaryMessages": [
			"i18n/temporary"
		],
		"WeirdGloopProjectNames": [
			"i18n/projectnames"
		],
		"WeirdGloopContactMessages": [
			"i18n/contact"
		]
	},
	"requires": {
		"MediaWiki": ">= 1.30.0"
	},
	"AutoloadClasses": {
		"WeirdGloopMessagesHooks": "WeirdGloopMessages.hooks.php"
	},
	"AutoloadNamespaces": {
		"WeirdGloopMessages\\ResourceLoaderModules\\": "includes/modules/"
	},
	"ResourceModules": {
		"wg.darkmode": {
			"class": "WeirdGloopMessages\\ResourceLoaderModules\\DarkmodeStyleModule"
		}
	},
	"Hooks": {
		"MessageCache::get": [
			"WeirdGloopMessagesHooks::onMessageCacheGet"
		],
		"SkinCopyrightFooter": [
			"WeirdGloopMessagesHooks::onSkinCopyrightFooter"
		],
		"SkinTemplateOutputPageBeforeExec": [
			"WeirdGloopMessagesHooks::onSkinTemplateOutputPageBeforeExec"
		],
		"TorBlockBlockedMsg": [
			"WeirdGloopMessagesHooks::onTorBlockBlockedMsg"
		],
		"GlobalBlockingBlockedIpMsg": [
			"WeirdGloopMessagesHooks::onGlobalBlockingBlockedIpMsg"
		],
		"GlobalBlockingBlockedIpXffMsg": [
			"WeirdGloopMessagesHooks::onGlobalBlockingBlockedIpXffMsg"
		],
		"UploadForm:initial": [
			"WeirdGloopMessagesHooks::onUploadFormInitial"
		],
		"getUserPermissionsErrors": [
			"WeirdGloopMessagesHooks::ongetUserPermissionsErrors"
		],
		"BeforePageDisplay": [
			"WeirdGloopMessagesHooks::onBeforePageDisplay"
		],
		"OutputPageBodyAttributes": [
			"WeirdGloopMessagesHooks::onOutputPageBodyAttributes"
		],
		"ContactForm": [
			"WeirdGloopMessagesHooks::onContactPage"
		]
	},
	"config": {
		"_prefix": "wgl",
		"SendDetailsWithContactPage": true,
		"EnableLoadingDarkmode": true,
		"EnableSearchboxMetadata": true,
		"ProtectSiteInterface": true,
		"RequireLicensesToUpload": true,
		"EnableMessageOverrides": true,
		"AddFooterLinks": true
	},
	"manifest_version": 1
}
