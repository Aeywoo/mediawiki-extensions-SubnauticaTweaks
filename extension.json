{
	"name": "GloopTweaks",
	"version": "1.1.0",
	"author": [
		"[https://twitter.com/jaydenkieran Jayden Bailey]",
		"TehKittyCat",
		"..."
	],
	"url": "https://weirdgloop.org",
	"descriptionmsg": "glooptweaks-desc",
	"license-name": "GPL-2.0+",
	"type": "other",
	"MessagesDirs": {
		"Analytics": [
			"i18n/analytics"
		],
		"WeirdGloopContactMessages": [
			"i18n/contact"
		],
		"WeirdGloopMessages": [
			"i18n/weirdgloop"
		],
		"WeirdGloopOverrides": [
			"i18n/weirdgloopoverrides"
		],
		"WeirdGloopProjectNames": [
			"i18n/projectnames"
		],
		"WeirdGloopTemporaryMessages": [
			"i18n/temporary"
		]
	},
	"requires": {
		"MediaWiki": ">= 1.35.0"
	},
	"AutoloadClasses": {
		"GenerateGitInfoCache": "maintenance/generateGitInfoCache.php",
		"GloopPreAuthenticationProvider": "includes/sfs/GloopPreAuthenticationProvider.php",
		"GloopStopForumSpam": "includes/sfs/GloopStopForumSpam.php",
		"GloopTweaksHooks": "GloopTweaksHooks.php",
		"GloopTweaksUtils": "GloopTweaksUtils.php"
	},
	"callback": "GloopTweaksHooks::onRegistration",
	"AutoloadNamespaces": {
		"GloopTweaks\\ResourceLoaderModules\\": "includes/modules/"
	},
	"ResourceModules": {
		"wg.darkmode": {
			"class": "GloopTweaks\\ResourceLoaderModules\\DarkmodeStyleModule"
		},
		"wg.readermode": {
			"styles": [
				"includes/css/vector-readermode.css"
			]
		}
	},
	"ResourceFileModulePaths": {
    "localBasePath": "",
    "remoteExtPath": "WeirdGloopMessages"
	},
	"Hooks": {
		"BeforePageDisplay": "GloopTweaksHooks::onBeforePageDisplay",
		"ContactForm": "GloopTweaksHooks::onContactPage",
		"GetLocalURL::Article": "GloopTweaksHooks::onGetLocalURLArticle",
		"GetLocalURL::Internal": "GloopTweaksHooks::onGetLocalURLInternal",
		"GlobalBlockingBlockedIpMsg": "GloopTweaksHooks::onGlobalBlockingBlockedIpMsg",
		"GlobalBlockingBlockedIpXffMsg": "GloopTweaksHooks::onGlobalBlockingBlockedIpXffMsg",
		"MessageCache::get": "GloopTweaksHooks::onMessageCacheGet",
		"OpenSearchUrls": "GloopTweaksHooks::onOpenSearchUrls",
		"OutputPageBodyAttributes": "GloopTweaksHooks::onOutputPageBodyAttributes",
		"PageSaveComplete": "GloopTweaksHooks::onPageSaveComplete",
		"SkinAddFooterLinks": "GloopTweaksHooks::onSkinAddFooterLinks",
		"SkinCopyrightFooter": "GloopTweaksHooks::onSkinCopyrightFooter",
		"TitleSquidURLs": "GloopTweaksHooks::onTitleSquidURLs",
		"TorBlockBlockedMsg": "GloopTweaksHooks::onTorBlockBlockedMsg",
		"UploadForm:initial": "GloopTweaksHooks::onUploadFormInitial",
		"UserGetRightsRemove": "GloopTweaksHooks::onUserGetRightsRemove",
		"getUserPermissionsErrors": "GloopTweaksHooks::ongetUserPermissionsErrors"
	},
	"config": {
		"_prefix": "wgl",
		"AddFooterLinks": true,
		"CentralDB": "metawiki",
		"EnableLoadingDarkmode": true,
		"EnableLoadingReadermode": true,
		"EnableMessageOverrides": true,
		"EnableMobileVariant": true,
		"EnableSearchboxMetadata": true,
		"ProtectSiteInterface": true,
		"RequireLicensesToUpload": true,
		"SendDetailsWithContactPage": true,
		"SensitiveRights": []
	},
	"AvailableRights": [
		"editinterfacesite",
		"bypassgloopspam"
	],
	"manifest_version": 1
}
