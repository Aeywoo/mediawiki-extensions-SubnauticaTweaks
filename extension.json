{
	"name": "GloopTweaks",
	"version": "1.0.1",
	"author": [
		"[https://twitter.com/jaydenkieran Jayden Bailey]",
		"TehKittyCat",
		"..."
	],
	"url": "https://weirdgloop.org",
	"descriptionmsg": "glooptweaks-desc",
	"license-name": "GPL-2.0+",
	"type": "other",
	"MessagesDirs": {
		"WeirdGloopMessages": [
			"i18n/weirdgloop"
		],
		"WeirdGloopOverrides": [
			"i18n/weirdgloopoverrides"
		],
		"WeirdGloopTemporaryMessages": [
			"i18n/temporary"
		],
		"WeirdGloopProjectNames": [
			"i18n/projectnames"
		],
		"WeirdGloopContactMessages": [
			"i18n/contact"
		],
		"Analytics": [
			"i18n/analytics"
		]
	},
	"requires": {
		"MediaWiki": ">= 1.35.0"
	},
	"AutoloadClasses": {
		"GloopTweaksHooks": "GloopTweaks.hooks.php",
		"GloopTweaksUtils": "GloopTweaksUtils.php",
		"GenerateGitInfoCache": "maintenance/generateGitInfoCache.php"
	},
	"AutoloadNamespaces": {
		"GloopTweaks\\ResourceLoaderModules\\": "includes/modules/"
	},
	"ResourceModules": {
		"wg.darkmode": {
			"class": "GloopTweaks\\ResourceLoaderModules\\DarkmodeStyleModule"
		},
		"wg.readermode": {
			"styles": [
				"includes/css/vector-readermode.css"
			]
		}
	},
	"ResourceFileModulePaths": {
    "localBasePath": "",
    "remoteExtPath": "WeirdGloopMessages"
	},
	"Hooks": {
		"MessageCache::get": [
			"GloopTweaksHooks::onMessageCacheGet"
		],
		"PageSaveComplete": [
			"GloopTweaksHooks::onPageSaveComplete"
		],
		"SkinCopyrightFooter": [
			"GloopTweaksHooks::onSkinCopyrightFooter"
		],
		"SkinTemplateOutputPageBeforeExec": [
			"GloopTweaksHooks::onSkinTemplateOutputPageBeforeExec"
		],
		"TorBlockBlockedMsg": [
			"GloopTweaksHooks::onTorBlockBlockedMsg"
		],
		"GlobalBlockingBlockedIpMsg": [
			"GloopTweaksHooks::onGlobalBlockingBlockedIpMsg"
		],
		"GlobalBlockingBlockedIpXffMsg": [
			"GloopTweaksHooks::onGlobalBlockingBlockedIpXffMsg"
		],
		"UploadForm:initial": [
			"GloopTweaksHooks::onUploadFormInitial"
		],
		"getUserPermissionsErrors": [
			"GloopTweaksHooks::ongetUserPermissionsErrors"
		],
		"BeforePageDisplay": [
			"GloopTweaksHooks::onBeforePageDisplay"
		],
		"OpenSearchUrls": [
			"GloopTweaksHooks::onOpenSearchUrls"
		],
		"OutputPageBodyAttributes": [
			"GloopTweaksHooks::onOutputPageBodyAttributes"
		],
		"ContactForm": [
			"GloopTweaksHooks::onContactPage"
		],
		"GetLocalURL::Article": [
			"GloopTweaksHooks::onGetLocalURLArticle"
		],
		"GetLocalURL::Internal": [
			"GloopTweaksHooks::onGetLocalURLInternal"
		]
	},
	"config": {
		"_prefix": "wgl",
		"CentralDB": "metawiki",
		"SendDetailsWithContactPage": true,
		"EnableLoadingDarkmode": true,
		"EnableSearchboxMetadata": true,
		"ProtectSiteInterface": true,
		"RequireLicensesToUpload": true,
		"EnableMessageOverrides": true,
		"AddFooterLinks": true,
		"EnableLoadingReadermode": true
	},
	"AvailableRights": [
		"editinterfacesite"
	],
	"manifest_version": 1
}
